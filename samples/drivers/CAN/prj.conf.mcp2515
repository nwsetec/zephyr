# CAN + MCP2515 config
CONFIG_SPI=y
CONFIG_CAN=y
CONFIG_CAN_INIT_PRIORITY=80
CONFIG_CAN_1=y
CONFIG_CAN_STM32=n
CONFIG_CAN_MCP2515=y
CONFIG_GPIO_LED_DEV="GPIO_0"
CONFIG_PIN_LED_1=17
CONFIG_GPIO_BUTTON_DEV="GPIO_0"
CONFIG_PIN_USER_BUTTON=13
CONFIG_LOG=y
# fix - not directly assignable CONFIG_CAN_LOG_LEVEL=4

CONFIG_CAN_MCP2515_OSC_FREQ=16000000
CONFIG_CAN_PHASE_SEG1=5
CONFIG_CAN_PHASE_SEG2=4
CONFIG_CAN_PROP_SEG=6
CONFIG_CAN_SJW=2

#CONFIG_ASSERT=y

CONFIG_SPI_1_NRF_SPI=y


CONFIG_LOOPBACK_MODE=y
